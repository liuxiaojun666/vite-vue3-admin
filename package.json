{
  "name": "vite-vue3-admin",
  "version": "0.0.0",
  "scripts": {
    "start": "nodemon",
    "predev": "run-p check-versions download:iconfont",
    "dev": "vite",
    "prebuild": "run-p check-versions download:iconfont",
    "build": "pnpm run build:prod",
    "build:prod": "run-p type-check build-only",
    "build:test": "run-p type-check build-test",
    "build:gh-pages": "run-p type-check build-gh-pages",
    "preview": "vite preview --port 4173",
    "build-only": "vite build",
    "build-test": "vite build --mode test",
    "build-gh-pages": "vite build --mode gh-pages",
    "check-versions": "ts-node ./scripts/check-versions",
    "init:localConfig": "ts-node ./scripts/initLocalConfig",
    "download:iconfont": "ts-node -T ./scripts/iconDownload",
    "type-check": "vue-tsc --noEmit",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "chmod +x ./scripts/only-pnpm-check.sh && ./scripts/only-pnpm-check.sh",
    "rm": "rm -rf node_modules/ && rm -rf package-lock.json && rm -rf yarn.lock && rm -rf pnpm-lock.yaml",
    "rm-debug": "ts-node ./scripts/rm-debug",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .eslintignore",
    "prepare": "husky install && npm run init:localConfig"
  },
  "dependencies": {
    "@vueuse/core": "^9.5.0",
    "axios": "^1.1.3",
    "date-fns": "^2.29.3",
    "element-plus": "^2.2.19",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.21",
    "qs": "^6.11.0",
    "ts-node": "^10.9.1",
    "vue": "^3.2.38",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.1.0",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/lodash": "^4.14.187",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^16.18.0",
    "@types/nprogress": "^0.2.0",
    "@types/qs": "^6.9.7",
    "@types/request": "^2.48.8",
    "@vitejs/plugin-legacy": "^2.3.0",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vitejs/plugin-vue-jsx": "^2.0.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "commitizen": "^4.2.5",
    "commitlint": "^17.1.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "lodash": "^4.17.21",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "request": "^2.88.2",
    "sass": "^1.56.0",
    "terser": "^5.15.1",
    "tslib": "^2.4.0",
    "typescript": "~4.7.4",
    "unplugin-auto-import": "^0.11.4",
    "unplugin-element-plus": "^0.4.1",
    "unplugin-vue-components": "^0.22.9",
    "vite": "^3.0.9",
    "vite-plugin-checker": "^0.5.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-html-version": "^0.0.3",
    "vite-plugin-top-level-await": "^1.2.1",
    "vue-tsc": "^1.0.9",
    "wget": "^0.0.1"
  },
  "engines": {
    "node": "~16.18.0"
  }
}
